{
  "name": "treefolder",
  "version": "1.0.0",
  "packageManager": "pnpm@10.28.2",
  "description": "A node.js CLI utility to display a tree representation (treeview) of a file &amp; folder structure.",
  "author": "khatastroffik",
  "license": "MIT",
  "keywords": [
    "tree",
    "treeview",
    "node.js",
    "khatastroffik",
    "treefolder",
    "folder",
    "structure",
    "CLI"
  ],
  "main": "dist/index.js",
  "bin": {
    "tfold": "./dist/index.js"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "prepare": "husky",
    "lint": "eslint .",
    "lintix": "eslint . --fix",
    "clean": "rimraf ./dist",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "pnpm clean && tsc",
    "start": "node .",
    "postpublish": "git push --tags",
    "try-commit": "git hook run pre-commit",
    "tsx": "tsx"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-jest": "^29.12.1",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "lint-staged": {
    "*.ts": "pnpm lintix"
  }
}
